# Ignore Python cache files
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore virtual environments if any
venv/
env/
.venv/
.env/

# Ignore git folder
.git
.gitignore

# Ignore local data (you mount this at runtime anyway)
data/input/*
data/output/*

# Ignore Docker files that don't need to be in the image
Dockerfile
.dockerignore

# Ignore logs or temp files
*.log
*.tmp
